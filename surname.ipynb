   "source": [
    "def surname(name1,name2): # note that name1 and name2 should already be hashed values\n",
    "    if name1 <= name2:\n",
    "        return md5((name1+name2).encode()).hexdigest()\n",
    "    else:\n",
    "        return md5((name2+name1).encode()).hexdigest()           "
   ]
  }
